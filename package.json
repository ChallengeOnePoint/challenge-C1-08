{
  "name": "react-redux",
  "description": "what i need to start a react-redux app",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --config webpack-dev.config.js",
    "predist": "rm -fr dist && mkdir dist && mkdir dist/js && mkdir dist/css",
    "dist": "npm run eslint && webpack && npm run dist:css && npm run dist:html && npm run dist:assets",
    "dist:css": "sass public/styles/base.scss | uglifycss > dist/css/style.css",
    "dist:html": "htmlprocessor public/index.html -o dist/index.html",
    "dist:assets": "cp -r public/styles/fonts dist/css/",
    "test:ut": "mocha --compilers js:babel-core/register --require public/scripts/specs/dom-config.js public/scripts/specs/config.js public/**/*.spec.js --recursive",
    "test:browser": "webpack-dev-server --config webpack-test.config.js --port 8081",
    "watch:js": "nodemon -L --watch public -e jsx,js -x \"npm run test:ut && npm run jscs\"",
    "eslint": "./node_modules/.bin/eslint public",
    "start": "node server/"
  },
  "dependencies": {
    "body-parser": "^1.14.2",
    "express": "4.13.4",
    "form-serialize": "^0.7.0",
    "lodash": "4.0.0",
    "mongoose": "^4.3.6",
    "normalize.scss": "0.1.0",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "react-router": "2.0.0-rc5",
    "redux": "3.0.5",
    "reqwest": "2.0.5",
    "winston": "2.1.1"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-1": "6.3.13",
    "chai": "3.4.1",
    "chai-dom": "1.4.0",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "4.0.0",
    "file-loader": "0.8.5",
    "htmlprocessor": "0.2.0",
    "jsdom": "7.2.2",
    "mocha": "2.3.4",
    "mocha-loader": "0.7.1",
    "node-sass": "3.4.2",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.0.1",
    "sass-loader": "3.1.2",
    "sinon": "1.17.2",
    "style-loader": "0.13.0",
    "uglify-loader": "^1.3.0",
    "uglifycss": "0.0.20",
    "url-loader": "0.5.7",
    "webpack": "1.12.11",
    "webpack-dev-server": "1.14.1"
  }
}
